{
  "name": "quickup-ui/1.0",
  "type": "project",
  "description": "The QuickUp is a base framework with user interface for starting a simple project or a test task.",
  "keywords": [
    "framework",
    "quickup"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Irina Sharga",
      "email": "irina.sharha@gmail.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\Entity\\": "app/entity",
      "App\\Model\\": "app/model"
    },
    "classmap": [
      "app"
    ],
    "files": [
      "app/config/main.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "8.0.*",
    "ext-json": "*",
    "ext-pdo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
